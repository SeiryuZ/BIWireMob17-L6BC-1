<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.get_table.biwiremob17_l6bc_1.HomePage">

    <RelativeLayout
        android:layout_width="375dp"
        android:layout_height="500dp"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <SearchView
            android:layout_width="368dp"
            android:layout_height="50dp"
            android:onClick="RestaurantSearchPressed"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="16dp"
            android:id="@+id/searchView3">

        </SearchView>

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="168dp"
            android:layout_height="159dp"
            android:layout_weight="1"
            android:onClick="RestaurantPressed"
            app:srcCompat="@drawable/pizza"
            tools:layout_editor_absoluteX="8dp"
            tools:layout_editor_absoluteY="77dp"
            android:layout_alignTop="@+id/imageButton2"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="168dp"
            android:layout_height="159dp"
            android:layout_weight="1"
            android:onClick="RestaurantPressed"
            app:srcCompat="@drawable/japanese"
            tools:layout_editor_absoluteX="188dp"
            tools:layout_editor_absoluteY="77dp"
            android:layout_marginTop="16dp"
            android:layout_below="@+id/searchView3"
            android:layout_alignEnd="@+id/searchView3" />

    </RelativeLayout>

</android.support.constraint.ConstraintLayout>
